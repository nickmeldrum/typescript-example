{
  "name": "typescript-example",
  "version": "1.0.0",
  "description": "a basic typescript setup example",
  "main": "dist/index.js",
  "scripts": {
    "prestart": "tsc",
    "start": "node dist/index.js",
    "test": "jest --coverage",
    "watch": "yarn test --watch --notify"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coverageDirectory": "coverage"
  },
  "author": "Nick Meldrum <nick@nickmeldrum.com> (https://nickmeldrum.com)",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "jest": "^24.7.1",
    "ts-jest": "^24.0.2",
    "typescript": "^3.4.4"
  }
}
